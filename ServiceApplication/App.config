<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <appSettings>
    <add key="DataPath" value="data.xml"/>
    <add key="Logging" value="true"/>
    <add key="UserServiceMode" value="master"/>
    <add key="IP" value="127.0.0.1"/>
    <add key="Port" value="10000"/>
    <!--split addresses using ';'-->
    <add key="Slaves" value="127.0.0.1:10000;127.0.0.1:10001"/>
  </appSettings>
</configuration>
